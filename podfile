platform :ios, '11.2'

target 'Maple' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

project 'Maple.xcodeproj'
pod 'AFNetworking'
pod 'Fabric'
# Add the Firebase pod for Google Analytics
pod 'Firebase/Analytics'

# Add the pods for any other Firebase products you want to use in your app
# For example, to use Firebase Authentication and Cloud Firestore
pod 'Firebase/Auth'
pod 'Firebase/Firestore'

pod 'Firebase/Firestore'
pod 'FirebaseMessaging' 
# Only pull in Firestore features
pod 'FirebaseUI/Firestore', '~> 8.0'

# Only pull in Database features
pod 'FirebaseUI/Database', '~> 8.0'

# Only pull in Storage features
pod 'FirebaseUI/Storage', '~> 8.0'

# Only pull in Auth features
pod 'FirebaseUI/Auth', '~> 8.0'

# Only pull in Facebook login features
pod 'FirebaseUI/Facebook', '~> 8.0'

# Only pull in Google login features
pod 'FirebaseUI/Google', '~> 8.0'

pod 'LocationPicker'
pod 'GooglePlacePicker'
pod 'GooglePlaces'
pod 'GoogleMaps'
pod 'GoogleSignIn'
pod 'BadgeSwift', '~> 5.0'
pod 'Fabric'
pod 'Mapbox-iOS-SDK', '~> 5.5.0'
pod 'MapboxNavigation', '~> 0.38.2'
pod 'Crashlytics'
pod 'InstantSearch-Core-Swift'
pod 'Reachability', '~> 3.2'
pod 'ImagePicker', '~> 3.1'
pod 'Lightbox'
pod 'Hue'
pod 'Gallery'
pod 'SVProgressHUD'
pod 'MaterialComponents'
pod 'CropViewController'
pod 'JGProgressHUD','~>2.0.3'
pod 'Alamofire', '~> 4.5'
pod 'SDWebImage', '~> 5.0'
pod 'ActiveLabel'
end

